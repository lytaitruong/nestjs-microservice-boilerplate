{
  "containerDefinitions": [
    {
      "name": "nestjs-book-api-uat",
      "image": "044043080677.dkr.ecr.ap-southeast-1.amazonaws.com/nestjs-book-api-uat",
      "cpu": 1024,
      "memory": 2048,
      "memoryReservation": 2048,
      "portMappings": [
        {
          "name": "nestjs-book-api-uat-3002-tcp",
          "containerPort": 3002,
          "hostPort": 3002,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::nestjs-microservice-ecs-configuration/uat/book-api/.env",
          "type": "s3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/nestjs-microservice-task-definition-uat",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "family": "nestjs-microservice-task-definition-uat",
  "taskRoleArn": "arn:aws:iam::044043080677:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::044043080677:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "ephemeralStorage": {
    "sizeInGiB": 100
  },
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": [
    {
      "key": "environment",
      "value": "uat"
    }
  ]
}
